{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.11"
  },
  "regions": ["iad1"],
  "functions": {
    "api/**/*.py": {
      "maxDuration": 10,
      "memory": 512
    }
  }
}
